border OCu(t = L2, L1) {x = t; y = l; label = 7;};
border OCl(t = l, l0) {x = L1; y = t; label = 8;};
mesh OmegaC = buildmesh(OCd(n) + OCr(n) + OCu(n) + OCl(n));
// Omega P
border OPd(t = L2, L) {x = t; y = l0; label = 9;};
