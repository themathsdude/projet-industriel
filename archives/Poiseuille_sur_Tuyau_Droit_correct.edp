border OPu(t = L, L2) {x = t; y = l; label = 11;};
border OPl(t = l, l0) {x = L2; y = t; label = 12;};
mesh OmegaP = buildmesh(OPd(n) + OPr(n) + OPu(n) + OPl(n));

